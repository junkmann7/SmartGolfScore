<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.tonosama.komoki.SimpleGolfScorer2"
    android:versionCode="58"
    android:versionName="2.6.2">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".SGSApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainTitle"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".editor.ScoreEditor"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".setting.SettingsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".viewer.ScoreViewer"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".history.HistoryActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".history.DroidLetterActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.GrayTranslucent" />
        <activity
            android:name=".viewer.GraphActivity"
            android:icon="@drawable/icon_type_line" />
    </application>

</manifest>