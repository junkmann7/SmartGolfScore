<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter>
    <Match>
        <Class name="~.*.R" />
        <Bug code="Nm,MS,NP"/>
    </Match>
    <Match>
        <Class name="~.*\.R\$.*"/>
        <Bug code="Nm,MS,NP"/>
    </Match>
    <Match>
        <Class name="~.*\.Manifest"/>
        <Bug code="Nm"/>
    </Match>
    <Match>
        <Class name="~.*\.Manifest\$.*"/>
        <Bug code="Nm"/>
    </Match>
</FindBugsFilter>